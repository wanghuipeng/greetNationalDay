	});
	//输入验证码
		$(".span_code").click(function(){ 
	    	$(".hide_code").focus();
	    }) 
	  $(".hide_code").keyup(function(){
    	var strCode = $(this).val();
    	$(".number-input li").each(function(i,j){
    		if(i>strCode.length-1){
    			$(j).find(".span_code").html('');
    		}
    	})
    	$(".number-input li").eq(strCode.length-1).find(".span_code").html(strCode.substring(strCode.length-1,strCode.length));
    	if(strCode.length<=3){
	    	$(".span_code").removeClass("current");
	    	$(".number-input li").eq(strCode.length).find(".span_code").addClass("current");
    	}
    	if(strCode.length==4){
    		$("input[name=serializeCode]").val(strCode);
    		var resume = $("#greetInfo").serialize();
			console.log(resume)
			$.post("/localApi/act/eleven/elevenSignup",resume, function (data) {
		    	  if(data=="success"){
		    		  $(".login-success").removeClass("hide");
		    		  timeshow();
		          }else if(data == "failure"){
		        	 new Message().show({msg:'您输入的验证码不正确!','type':1});
		          }else if(data=="error"){
		        	  new Message().show({msg:'页面已失效，请刷新后重试！','type':1,'fn':function(){window.location.reload();}});
		          }
            });
    	}
    }) 
        //选择地点点击展示省
        $(".form-g-text").click(function () {
            $.get("/api/base/findProvinceList", function (data) {
                $('#provinceLists').html(template("province", data));
                /*省二级的市选择*/
                var provinceName;
                var cityName;
                $('.ul_province li').click(function () {
                    $(".hot-cities span").removeClass('active');
                    cityName = '';
                    var provinceId = $(this).attr("code");
                    provinceName = $(this).text();
                    $.get("/api/base/findCityListByProvinceId", {"provinceId": provinceId}, function (result) {
                        var result = $.parseJSON(result).data;

                        var lis = [];
                        $.each(result, function (i, j) {
                            lis.push('<li code="' + j.id + '">' + j.abbreviation + '</li>');
                        });
                        $(".ul_city_list").empty();
                        $(".ul_city_list").append(lis.join(''));
                        //点击选择某个城市后执行
                        $('.ul_city_list li').click(function () {
                            cityName = $(this).text();
                            $(this).parents('.linkage-select-panel').removeClass('on');
                            $('.form-g-text').html($.trim(provinceName) + " " + $.trim(cityName));
                            $("input[name='provinceId']").val(provinceId);
                            $("input[name='cityId']").val($(this).attr("code"));
                            vaild();
                        });
                    });
                });
            })
