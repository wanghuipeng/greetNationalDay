<script type="text/javascript">
	//图片预加载
(function() {
	//refreshRem
	function refreshRem() {
		var docEl = window.document.documentElement;
		var width = docEl.getBoundingClientRect().width;
		var rootSize = width / 10;
		docEl.style.fontSize = rootSize + 'px';
	}
	refreshRem();
	// 图片加载计算
	function loading(par) {
		var sum = par.srcArray.length || 0;
		var i = 0;
		if(sum != 0) {
			var len = sum - 1;
			var imgs = [];
			while(len >= 0) {
				imgs.push(new Image());
				imgs[i].src = par.srcArray[len];
				imgs[i].onload = par.fun || null;
				imgs[i].onerror = par.fun || null;
				i++;
				len--;
			}
		}
	}
	var par = {};
	par.srcArray = [
		'scene_French/img_1.png','scene_French/img_2.jpg',
		'scene_Japan/img_1.jpg',
		'scene_Maldives/img_1.jpg','scene_Maldives/img_2.jpg','scene_Maldives/img_3.jpg',
		'scene_Sahara/img_1.jpg','scene_Sahara/img_2.jpg',
		'scene_Tailand/img_1.png','scene_Tailand/img_2.jpg',
		'scene_Tibet/img_1.png','scene_Tibet/img_2.jpg','scene_Tibet/img_3.jpg',
		'scene_US/img_1.png','scene_US/img_2.png'
	];
	var href = window.location.host;
	for(var i = par.srcArray.length - 1; i >= 0; i--) {
		par.srcArray[i] = '../static/images/' + par.srcArray[i];
	};
	var loaded = 0;
	var len = par.srcArray.length;
	par.fun = function() {
		if(++loaded / len == 1) {
			$('.loading').hide(); 

		};
	};
	loading(par);
})();
</script>
