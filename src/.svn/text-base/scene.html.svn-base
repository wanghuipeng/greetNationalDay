<script>
	//取localstorage的base64和姓名
	var dataArr = [];
	var storageArr = window.localStorage;  
	for (var i=0, len = storageArr.length; i  <  len; i++){     
	  var key = storageArr.key(i);       
	  var value = storageArr.getItem(key);     
