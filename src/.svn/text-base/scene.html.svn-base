	/**验证数据*/
	function vaild() {
        var result = true;
        var name_reg = /((^[\u4E00-\u9FA5]{2,12}$)|(^[a-zA-Z]{3,24}$))/,
            phone_reg = /^1[3-9][0-9]{9}$/i;
        var name = $("#friend_name").val();
        var mobile = $("#input_phone").val();
        var jobTargetProvinceid = $("input[name='provinceId']").val();//选择城市
        var jobTargetCityid = $("input[name='cityId']").val();

        if (name == "" || name == undefined || name == null || !name_reg.test(name)) {//
            new Message().show({'msg': '姓名为2-12位的汉字或3-24位的英文！', 'type': 1, 'position': 'bottom'});
            $("#friend_name").focus();
            result = false;
        } else if (mobile != undefined && !phone_reg.test(mobile)) {
            new Message().show({'msg': '请输入正确的手机号', 'type': 1, 'position': 'bottom'});
            $("#input_phone").focus();
            result = false;
        } else if (jobTargetProvinceid == null || jobTargetProvinceid == "" || jobTargetCityid == null || jobTargetCityid == "") {
            new Message().show({'msg': '请选择期望的工作地点', 'type': 1, 'position': 'bottom'});
            result = false;
        } else {
            $(".form-list").find(".form-g-tip").hide();
        }
        return result;
    }
	/*3秒后跳转*/
	var _flag = 2;
   	var _timeOutFlag;
    function timeshow(){
	    if(_flag>0){
	      _timeOutFlag = setTimeout("timeshow()",1000);
	      $('.span_second').html(_flag);
	      _flag--;
	    }else{
	      clearTimeout(_timeOutFlag);
	      window.location.href = "http://m.youlanw.com/sh/";
	    };
    }

</script>
